    "prettier": "^3.6.2",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.6.3",
    "vitest": "^4.0.13"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write"
