    "cors": "^2.8.5",
    "cron": "^4.3.4",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "http-proxy": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "rotating-file-stream": "^3.2.7",
    "sequelize": "^6.37.7",
