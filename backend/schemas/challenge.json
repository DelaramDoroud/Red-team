{
  "$id": "challenge",
  "type": "object",
  "properties": {
    "status": {
      "enum": ["public", "private"],
      "default": "private"
    },
    "id": {
      "type": "integer"
    },
    "title": {
      "$ref": "shared-definitions#/definitions/notEmptyString"
    },
    "duration": {
      "type": "integer",
      "minimum": 1,
      "description": "Duration in minutes"
    },
    "startDatetime": {
      "type": "string",
      "format": "date-time"
    },
    "endDatetime": {
      "type": "string",
      "format": "date-time"
    },
    "peerReviewStartDate": {
      "type": "string",
      "format": "date-time"
    },
    "peerReviewEndDate": {
      "type": "string",
      "format": "date-time"
    },
    "allowedNumberOfReview": {
      "type": "integer",
      "minimum": 0
    },
    "matchSettingIds": {
      "type": "array",
      "items": {
        "type": "integer"
      },
      "uniqueItems": true
    }
  },
  "required": [
    "title",
    "duration",
    "startDatetime",
    "endDatetime",
    "peerReviewStartDate",
    "peerReviewEndDate"
  ],
  "additionalProperties": false
}
